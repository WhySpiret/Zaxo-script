
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/DenDenZZZ/Orion-UI-Library/refs/heads/main/source')))()

local Window = OrionLib:MakeWindow({
    Name = "Zaxo - 1.0",
    HidePremium = false,
    SaveConfig = false,           
    ConfigFolder = nil,       
    IntroEnabled = true,
    IntroText = "Welcome, " .. game.Players.LocalPlayer.Name,
    IntroIcon = "rbxassetid://82778010291487"	
})

local Tab = Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://140587862516598",                 -- must use image asset id
    PremiumOnly = false
})

-- Adding a slider for test
Tab:AddSlider({
    Name = "Walk Speed",
    Min = 36,                                              -- Minimum value for the slider
    Max = 150,                                            -- Maximum value for the slider
    Default = 36,                                         -- Default value of the slider
    Color = Color3.fromRGB(math.random(0, 255), math.random(0, 255), math.random(0, 255)),
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value  -- Updates the player's walk speed
    end
})

local Tab = Window:MakeTab({
    Name = "Player",
    Icon = "rbxassetid://8395621517",                 -- must use image asset id
    PremiumOnly = false
})

-- Adding a slider for test
Tab:AddSlider({
    Name = "Walk Speed",
    Min = 36,                                              -- Minimum value for the slider
    Max = 150,                                            -- Maximum value for the slider
    Default = 36,                                         -- Default value of the slider
    Color = Color3.fromRGB(180, 0, 0),
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value  -- Updates the player's walk speed
    end
})

Tab:AddButton({
    Name = "Jump - 50",
    Callback = function()
        game.WorkSpace.Gravity = 50
    end
})

Tab:AddButton({
    Name = "Default Jump",
    Callback = function()
        game WorkSpace.Gravity = 300
    end
})
-- Never forget to add this when using Orion Library
OrionLib:Init()
